    <!-- 错误 Error.log-->
    <appender name="ErrorLog" type="log4net.Appender.RollingFileAppender">
      <!--目录路径，可以是相对路径或绝对路径-->
      <param name="File" value="Agile_log" />
      <!--文件名，按日期生成文件夹-->
